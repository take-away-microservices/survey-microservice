{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "import/prefer-default-export": 0,
    "no-else-return": 0,
  },
  "env": {
    "node": true,
    "browser": false,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "extends": [
    "airbnb-base"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
